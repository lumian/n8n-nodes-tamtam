{
  "name": "n8n-nodes-tamtam",
  "version": "1.0.0",
  "description": "n8n community node for TamTam messenger integration",
  "keywords": [
    "n8n-community-node-package",
    "tamtam",
    "messenger",
    "chat",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/lumian/n8n-nodes-tamtam",
  "author": {
    "name": "lumian",
    "email": "lum@hionline.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lumian/n8n-nodes-tamtam.git"
  },
  "engines": {
    "node": ">=18.10",
    "npm": ">=9.5.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist/**/*"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TamTamApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TamTam/TamTam.node.js",
      "dist/nodes/TamTamTrigger/TamTamTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/request": "^2.48.8",
    "@types/request-promise-native": "^1.0.18",
    "gulp": "^4.0.2",
    "gulp-svgmin": "^4.1.0",
    "gulp-zip": "^6.0.0",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}