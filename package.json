{
  "name": "n8n-nodes-tamtam",
  "version": "1.0.0",
  "description": "n8n community node for TamTam messenger integration",
  "keywords": [
    "n8n-community-node-package",
    "tamtam",
    "messenger",
    "chat",
    "api"
  ],
  "license": "MIT",
  "homepage": "https://github.com/your-username/n8n-nodes-tamtam",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/n8n-nodes-tamtam.git"
  },
  "engines": {
    "node": ">=18.10",
    "npm": ">=9.5.0"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/TamTamApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/TamTam/TamTam.node.js",
      "dist/nodes/TamTamTrigger/TamTamTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.2",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}